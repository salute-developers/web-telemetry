{
  "name": "@salutejs/web-telemetry",
  "version": "2.0.10",
  "description": "Web performance monitoring",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/salute-developers/web-telemetry.git"
  },
  "license": "MIT",
  "author": "Salute Frontend Team <salute.developers@gmail.com>",
  "contributors": [],
  "type": "module",
  "exports": {
    "./lib": {
      "import": "./esm/presets/WebTelemetry.js",
      "require": "./lib/presets/WebTelemetry.js"
    },
    "./lib/types": {
      "import": "./esm/types.js",
      "require": "./lib/types.js"
    },
    "./lib/presets/*": {
      "import": "./esm/presets/*.js",
      "require": "./lib/presets/*.js"
    },
    "./lib/extra/*": {
      "import": "./esm/extra/*.js",
      "require": "./lib/extra/*.js"
    },
    "./lib/constants": {
      "import": "./esm/constants.js",
      "require": "./lib/constants.js"
    },
    "./lib/config": {
      "import": "./esm/config.js",
      "require": "./lib/config.js"
    },
    "./lib/helpers": {
      "import": "./esm/helpers.js",
      "require": "./lib/helpers.js"
    },
    "./lib/WebTelemetryTransport": {
      "import": "./esm/WebTelemetryTransport.js",
      "require": "./lib/WebTelemetryTransport.js"
    },
    "./lib/WebTelemetryBase": {
      "import": "./esm/WebTelemetryBase.js",
      "require": "./lib/WebTelemetryBase.js"
    }
  },
  "files": [
    "esm",
    "lib"
  ],
  "scripts": {
    "build": "npm run clean && tsc --erasableSyntaxOnly && npm run build:cjs",
    "build:cjs": "tsc -p tsconfig.cjs.json --erasableSyntaxOnly",
    "check-types": "tsc --noEmit --erasableSyntaxOnly",
    "clean": "rm -rf lib esm",
    "dev": "tsc --watch",
    "embed-version": "node embedVersion.js",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "eslint . --fix",
    "prepare": "npm run build && npm run embed-version",
    "prepublishOnly": "rm -rf ./lib ./esm && npm test && tsc",
    "release": "git diff && auto shipit",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "uuid": "11.1.0",
    "web-vitals": "4.2.4"
  },
  "devDependencies": {
    "@salutejs/client": "1.35.5",
    "@salutejs/eslint-config": "3.2.1",
    "@salutejs/prettier-config": "3.1.0",
    "@types/node": "22.13.9",
    "auto": "11.2.1",
    "eslint": "9.21.0",
    "glob": "11.0.1",
    "jsdom": "25.0.1",
    "prettier": "3.5.3",
    "typescript": "5.8.2",
    "vitest": "3.0.7"
  },
  "publishConfig": {
    "access": "public"
  }
}
