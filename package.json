{
  "name": "@salutejs/web-telemetry",
  "version": "2.0.10",
  "description": "Web performance monitoring",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/salute-developers/web-telemetry.git"
  },
  "license": "MIT",
  "author": "Salute Frontend Team <salute.developers@gmail.com>",
  "contributors": [],
  "type": "module",
  "exports": {
    ".": "./esm/presets/WebTelemetry.js",
    "./types": "./esm/types.js",
    "./presets/*": "./esm/presets/*.js",
    "./extra/*": "./esm/extra/*.js",
    "./constants": "./esm/constants.js",
    "./config": "./esm/config.js",
    "./helpers": "./esm/helpers.js",
    "./WebTelemetryTransport": "./esm/WebTelemetryTransport.js",
    "./WebTelemetryBase": "./esm/WebTelemetryBase.js"
  },
  "files": [
    "lib",
    "generateVersion.js",
    "version.js"
  ],
  "scripts": {
    "build": "npm run clean && tsc --erasableSyntaxOnly && tsc -p tsconfig.cjs.json --erasableSyntaxOnly",
    "build:cjs": "",
    "check-types": "tsc --noEmit  --erasableSyntaxOnly",
    "clean": "rm -rf lib esm",
    "dev": "tsc --watch",
    "fmt": "prettier --write .",
    "fmt:check": "prettier --check .",
    "postinstall": "node generateVersion.js",
    "lint": "eslint . --fix",
    "prepare": "npm run build",
    "prepublishOnly": "rm -rf ./lib ./esm && npm test && tsc",
    "release": "git diff && auto shipit",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "uuid": "11.1.0",
    "web-vitals": "4.2.4"
  },
  "devDependencies": {
    "@salutejs/client": "1.35.5",
    "@salutejs/eslint-config": "3.2.1",
    "@salutejs/prettier-config": "3.1.0",
    "@types/node": "22.13.9",
    "auto": "11.2.1",
    "eslint": "9.21.0",
    "jsdom": "25.0.1",
    "prettier": "3.5.3",
    "typescript": "5.8.2",
    "vitest": "3.0.7"
  },
  "publishConfig": {
    "access": "public"
  }
}
